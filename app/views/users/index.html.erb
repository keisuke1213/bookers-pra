<%= render 'userinfo', user: @user, book: @book %>

<h1>Users</h1>
<% @users.each do |user| %>
<table>
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
    </tr>
    <tbody>
      <tr>
        <td>
          <%= image_tag user.get_profile_image(100,100) %>
        </td>
        <td>
          <%= user.name %>
        </td>
        <td>
          <%= link_to "Show", user_path(user.id) %>
        </td>
      </tr>
    </tbody>
  </thead>
</table>
<% end %>